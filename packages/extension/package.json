{
  "name": "rhel-lightspeed",
  "displayName": "RHEL Lightspeed",
  "description": "Podman Desktop extension allowing to get help from RHEL Lightspeed",
  "version": "0.3.0-next",
  "icon": "icon.png",
  "publisher": "redhat",
  "license": "Apache-2.0",
  "engines": {
    "podman-desktop": ">=1.18.0"
  },
  "main": "./dist/main.js",
  "scripts": {
    "build": "vite build",
    "test": "vitest run --coverage",
    "test:watch": "vitest watch --coverage",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "format:fix": "prettier --write \"src/**/*.ts\"",
    "lint:check": "eslint . --ext js,ts,tsx",
    "lint:fix": "eslint . --fix --ext js,ts,tsx",
    "watch": "vite --mode development build -w"
  },
  "devDependencies": {
    "@podman-desktop/api": "1.20.2",
    "@types/node": "^22",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.30.1",
    "@vitest/coverage-v8": "^3.2.2",
    "eslint": "^9.32.0",
    "eslint-import-resolver-custom-alias": "^1.3.2",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-etc": "^2.0.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-redundant-undefined": "^1.0.0",
    "eslint-plugin-sonarjs": "^3.0.2",
    "prettier": "^3.6.1",
    "typescript": "5.8.3",
    "vite": "^7.0",
    "vitest": "^3.1"
  },
  "dependencies": {
    "ansi_up": "^6.0.5",
    "inversify": "^7.7.0",
    "reflect-metadata": "^0.2.2"
  }
}
